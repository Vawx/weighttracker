

<div class="row">
  <div class="col-xs-10 col-xs-offset-2">
    <div class="col-xs-11 col-xs-offset-1 foodList">
      <table style="width:100%">
        <% @foods.each do |f| %>
          <tr>
            <td><%= f.name %></td>
            <td><%= f.calories %></td>
          </tr>
        <%end%>
      </table>
    </div>
  </div>
</div>
